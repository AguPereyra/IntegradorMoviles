<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iua.agustinpereyra">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/brand_cow_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/brand_cow_icon_round"
        android:supportsRtl="true"
        android:theme="@style/CattleTheme">
        
        <activity android:name=".controller.welcomeviews.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".controller.cattleviews.CattleListActivity"/>
        <activity android:name=".controller.userviews.UserAccountActivity"/>
        <activity android:name=".controller.settingsviews.SettingsActivity"/>
        <activity android:name=".controller.helpviews.HelpActivity"/>
    </application>
</manifest>